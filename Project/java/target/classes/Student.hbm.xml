<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/orm/hbm">
    <class name ="com.company.day8.Student" table="student">
        <id name = "id" type="int">
            <column name="id"></column>
            <generator class="com.company.day8.CustomIdGenerator"></generator>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="roll_no" column="roll_no" type="int"/>
        <property name="university" column="university" type="string"/>


    </class>

</hibernate-mapping>



